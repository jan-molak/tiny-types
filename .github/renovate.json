{
  "extends": [
    ":dependencyDashboard",
    ":semanticPrefixFixDepsChoreOthers",
    ":prHourlyLimit2",
    ":prConcurrentLimit20",
    "group:monorepos",
    "group:recommended",
    "workarounds:typesNodeVersioning",
    "helpers:disableTypesNodeMajor",
    "helpers:pinGitHubActionDigests"
  ],
  "ignorePaths": [
    "**/node_modules/**"
  ],
  "rangeStrategy": "pin",
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch",
        "pin",
        "digest"
      ],
      "automerge": true
    }
  ]
}
